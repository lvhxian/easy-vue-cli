{
  "name": "easy-vue-cli",
  "version": "1.0.0",
  "description": "Weclome to easy-vue-cli",
  "main": "index.js",
  "scripts": {
    "build-client": "cross-env NODE_ENV=production webpack --config config/webpack.client.js",
    "build-server": "cross-env NODE_ENV=production webpack --config config/webpack.server.js",
    "clean": "rimraf build && rimraf server-build",
    "build": "npm run clean && npm run build-client && npm run build-server && npm run upload",
    "dev-client": "cross-env NODE_ENV=development webpack-dev-server --inline --color --config config/webpack.client.js",
    "dev-server": "nodemon server/app.js",
    "dev": "concurrently \"npm run dev-client\"",
    "upload": "node dev/devOss.js",
    "precommit": "npm run lint-fix",
    "start": "cross-env NODE_ENV=production node server/app.js",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/"
  },
  "author": "Toby",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "ejs": "^2.5.7",
    "fastclick": "^1.0.6",
    "koa": "^2.5.1",
    "koa-body": "^2.5.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.2",
    "koa-session": "^5.8.1",
    "sha1": "^1.1.1",
    "vue": "2.5.13",
    "vue-meta": "^1.4.3",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-vue-jsx": "3.5.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "5.1.3",
    "css-loader": "^0.28.11",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.14.12",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "2.0.9",
    "progress-bar-webpack-plugin": "^1.11.0",
    "qiniu": "^7.1.5",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.6.2",
